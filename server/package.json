{
  "name": "todos",
  "version": "1.0.0",
  "description": "todos app using type orm",
  "main": "app.js",
  "scripts": {
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migrationName -p",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node-dev --respawn ./src/app.ts"
  },
  "author": "Refaat Aish",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.44"
  }
}
